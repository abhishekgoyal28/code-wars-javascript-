			<h4 class="text-center">Number stored : <span id="numStored"><script>
				document.write(ns);
			</script></span></h4>
			
			<script>
			document.onkeydown = function(e) {
 		    switch (e.keyCode) {
        		case 37: 
        		    	changeclassl();
        		    	break;
       			case 38:
        	    		changeclassu();
        	    		break;
        		case 39:
        				changeclassr();
        		    	break;	
        		case 40:
        		   		changeclassd();
        		    	break;
        		case 13:
    				{
    					if(f == 0)
        					store();
        				else
        					swap();
    				}
    				break;
    		}
			};

			function changeclassr()
			{
				var nid = document.getElementsByClassName("maze-cell active")[0].id;
				nid++;
				if(nid < 26)
				{	
					document.getElementsByClassName("maze-cell active")[0].className = "maze-cell";
        			document.getElementById(nid).className = "maze-cell active";
        		}
			}
			function changeclassl()
			{
				var nid = document.getElementsByClassName("maze-cell active")[0].id;
				nid--;
				
				if(Number(nid) > 0)	
				{
					document.getElementsByClassName("maze-cell active")[0].className = "maze-cell";
        			document.getElementById(nid).className = "maze-cell active";
				}
			}
			function changeclassu()
			{
				var nid = document.getElementsByClassName("maze-cell active")[0].id;
				if(nid > 5)
				{
					nid -= 5;
					document.getElementsByClassName("maze-cell active")[0].className = "maze-cell";
        			document.getElementById(nid).className = "maze-cell active";
				}
			}
			function changeclassd() 
			{
				var nid = document.getElementsByClassName("maze-cell active")[0].id;
				var t = Number(Number(nid) + Number(5));
				if(nid < 21)
				{
					document.getElementsByClassName("maze-cell active")[0].className = "maze-cell";
        			document.getElementById(t).className = "maze-cell active";
				}
			}
			function store()
			{

				var nid = document.getElementsByClassName("maze-cell active")[0];
              	r1 = nid.getAttribute("data-rownum");
              	c1 = nid.getAttribute("data-colnum");
              	sid = nid.id;
              	ns = Number(m[r1-1][c1-1]);
				f = 1;
				document.getElementById("numStored").innerHTML=ns;
				document.getElementById(nid.id).innerHTML="";
			}
			function swap()
			{
				var nid = document.getElementsByClassName("maze-cell active")[0];
				r2 = nid.getAttribute("data-rownum");
              	c2 = nid.getAttribute("data-colnum");
              	var s = m[r2-1][c2-1];
              	m[r2-1][c2-1] = Number(ns);
              	m[r1-1][c1-1] = Number(s);
              	document.getElementById(sid).innerHTML = "<div>"+s+"</div>";
              	document.getElementById(nid.id).innerHTML="<div>"+ns+"</div>";
              	document.getElementById("numStored").innerHTML="none";	
              	f = 0;
			}
		</script>
		
